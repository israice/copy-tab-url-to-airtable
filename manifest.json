{
  "manifest_version": 3,
  "name": "Copy tab url to airtable",
  "version": "1.0",
  "description": "Chrome extention that copy tab url to airtable.",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://api.airtable.com/v0/app8EMe8KlU5dSbaS/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}